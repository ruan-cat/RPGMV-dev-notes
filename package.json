{
	"name": "@ruan-cat-docs/root",
	"private": true,
	"version": "1.0.0",
	"description": "笔记单仓根目录。",
	"license": "MIT",
	"type": "module",
	"packageManager": "pnpm@9.15.0",
	"engines": {
		"node": ">=22.14.0"
	},
	"scripts": {
		"build": "turbo build:docs",
		"deploy-vercel": "turbo do-deploy-vercel",
		"up-ruan-cat": "pnpm -F=* up @ruan-cat/*@latest",
		"git:dev-2-main": "git rebase dev main && pnpm run git:push",
		"git:main-2-dev": "git rebase main dev",
		"git:dev-2-vc": "git rebase dev vc && pnpm run git:push",
		"git:vc-2-dev": "git rebase vc dev",
		"clear:cache": "rimraf -g **/{dist,.turbo,.vercel,.cache,.temp}",
		"clear:deps": "rimraf -g **/{node_modules,package-lock.json,pnpm-lock.yaml}",
		"format": "prettier --write .",
		"test": "vitest --ui --watch --api.port=4000",
		"create-code-workspace": "node --import=tsx ./scripts/generate.code-workspace.ts --name=note笔记项目",
		"update-package": "pnpm dlx vp-update",
		"preinstall": "npm run corepack:pnpm",
		"old-preinstall": "npx only-allow pnpm && npm run corepack:pnpm",
		"corepack:pnpm": "corepack enable && corepack install",
		"git:fetch": "git fetch -p",
		"git:push": "git push --follow-tags",
		"deploy-vercel-scripts": "node --import=tsx ./scripts/vercel-deploy-tool.ts",
		"turbo:update-packages": "turbo docs:update-package && pnpm update-package",
		"on-github-action-flow": "pnpm run build",
		"build:docs:note": "turbo run do-build-note-doc",
		"build:docs:01star": "turbo run do-build-01star-doc"
	},
	"dependencies": {
		"@vercel/analytics": "^1.5.0",
		"@vueuse/core": "^10.11.1",
		"animate.css": "^4.1.1",
		"axios": "^1.8.3",
		"dayjs": "^1.11.13",
		"element-plus": "^2.9.6",
		"lodash-es": "^4.17.21",
		"node-localstorage": "^2.2.1",
		"pinia": "^2.3.1",
		"pinia-plugin-persistedstate": "^3.2.3",
		"uuid": "^9.0.1",
		"vue": "^3.5.13",
		"vue-json-excel3": "^0.0.9",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.317",
		"@iconify/vue": "^4.3.0",
		"@ruan-cat/commitlint-config": "^1.1.4",
		"@ruan-cat/domains": "^0.6.0",
		"@ruan-cat/generate-code-workspace": "^0.4.8",
		"@ruan-cat/utils": "^4.7.0",
		"@ruan-cat/vercel-deploy-tool": "^0.9.1",
		"@shikijs/twoslash": "^1.29.2",
		"@shikijs/vitepress-twoslash": "^1.29.2",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^22.13.10",
		"@types/uuid": "^10.0.0",
		"@vitejs/plugin-vue": "^5.2.3",
		"@vitest/ui": "^2.1.9",
		"cross-env": "^7.0.3",
		"cz-git": "^1.11.1",
		"moment": "^2.30.1",
		"only-allow": "^1.2.1",
		"pkg-types": "^1.3.1",
		"prettier": "^3.5.3",
		"prettier-plugin-lint-md": "^1.0.1",
		"rimraf": "^6.0.1",
		"tsx": "^4.19.3",
		"turbo": "^2.4.4",
		"typedoc": "^0.25.13",
		"typedoc-plugin-markdown": "^3.17.1",
		"typescript": "^5.8.2",
		"unplugin-auto-import": "^19.1.1",
		"unplugin-vue-components": "^28.4.1",
		"vercel": "39.3.0",
		"vite": "^6.2.2",
		"vitest": "^2.1.9"
	}
}
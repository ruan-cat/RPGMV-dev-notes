{
	"name": "@ruan-cat-docs/root",
	"private": true,
	"version": "1.0.0",
	"description": "笔记单仓根目录。",
	"license": "MIT",
	"type": "module",
	"packageManager": "pnpm@9.10.0",
	"engines": {
		"vscode": "^1.22.0",
		"node": ">=20.15.1",
		"pnpm": ">=9.5.0"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm && npm run corepack:pnpm",
		"corepack:pnpm": "corepack enable && corepack install",
		"test": "vitest --ui --watch --api.port=3000",
		"prettier": "prettier --write .",
		"git:fetch": "git fetch -p",
		"git:dev-2-main": "git rebase dev main",
		"git:main-2-dev": "git rebase main dev",
		"git:dev-2-vc": "git rebase dev vc",
		"git:vc-2-dev": "git rebase vc dev",
		"up-ruan-cat": "pnpm -F=* up @ruan-cat/*@latest",
		"update-package": "pnpm dlx vp-update",
		"turbo:update-packages": "turbo docs:update-package && pnpm update-package",
		"rm:node_modules": "pnpm -r rm:node_modules && rimraf node_modules pnpm-lock.yaml",
		"clear": "pnpm -F=* rimraf dist .turbo .vercel .cache .temp",
		"turbo:build-docs": "turbo build:docs",
		"on-github-action-flow": "pnpm turbo:build-docs",
		"code-workspace:create": "node --import=tsx ./scripts/generate.code-workspace.ts --name=note笔记项目",
		"deploy-vercel": "node --import=tsx ./scripts/vercel-deploy-tool.ts"
	},
	"dependencies": {
		"@vercel/analytics": "^1.4.1",
		"@vueuse/core": "^10.11.1",
		"animate.css": "^4.1.1",
		"axios": "^1.7.9",
		"dayjs": "^1.11.13",
		"element-plus": "^2.9.3",
		"lodash-es": "^4.17.21",
		"node-localstorage": "^2.2.1",
		"pinia": "^2.3.1",
		"pinia-plugin-persistedstate": "^3.2.3",
		"uuid": "^9.0.1",
		"vue": "^3.5.13",
		"vue-json-excel3": "^0.0.9",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@iconify/json": "^2.2.302",
		"@iconify/vue": "^4.3.0",
		"@ruan-cat/commitlint-config": "^0.2.0",
		"@ruan-cat/domains": "^0.1.4",
		"@ruan-cat/generate-code-workspace": "^0.3.0",
		"@ruan-cat/utils": "^1.5.0",
		"@ruan-cat/vercel-deploy-tool": "^0.6.1",
		"@ruan-cat/vuepress-preset-config": "^0.1.19",
		"@shikijs/twoslash": "^1.29.2",
		"@shikijs/vitepress-twoslash": "^1.29.2",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^22.13.1",
		"@types/uuid": "^10.0.0",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vitest/ui": "^2.1.9",
		"@vuepress/bundler-vite": "2.0.0-rc.19",
		"@vuepress/plugin-blog": "2.0.0-rc.74",
		"@vuepress/plugin-docsearch": "2.0.0-rc.74",
		"@vuepress/plugin-register-components": "2.0.0-rc.66",
		"@vuepress/plugin-slimsearch": "2.0.0-rc.74",
		"cross-env": "^7.0.3",
		"cz-git": "^1.11.0",
		"mermaid": "^11.4.1",
		"moment": "^2.30.1",
		"only-allow": "^1.2.1",
		"pkg-types": "^1.3.1",
		"prettier": "^3.4.2",
		"prettier-plugin-lint-md": "^1.0.1",
		"rimraf": "^6.0.1",
		"sass-embedded": "^1.83.4",
		"tsx": "^4.19.2",
		"turbo": "^2.3.4",
		"typedoc": "^0.25.13",
		"typedoc-plugin-markdown": "^3.17.1",
		"typescript": "^5.7.3",
		"unplugin-auto-import": "^19.0.0",
		"unplugin-vue-components": "^28.0.0",
		"vercel": "39.3.0",
		"vite": "^6.0.11",
		"vitest": "^2.1.9",
		"vuepress": "2.0.0-rc.19",
		"vuepress-plugin-md-enhance": "2.0.0-rc.71",
		"vuepress-plugin-typedoc": "0.13.0",
		"vuepress-theme-hope": "2.0.0-rc.71"
	}
}
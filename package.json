{
	"name": "@ruan-cat-docs/root",
	"private": true,
	"version": "1.0.0",
	"description": "笔记单仓根目录。",
	"license": "MIT",
	"type": "module",
	"packageManager": "pnpm@9.10.0",
	"engines": {
		"vscode": "^1.22.0",
		"node": ">=20.15.1",
		"pnpm": ">=9.5.0"
	},
	"config": {
		"commitizen": {
			"path": "node_modules/cz-git"
		}
	},
	"scripts": {
		"preinstall": "npx only-allow pnpm && npm run corepack:pnpm",
		"corepack:pnpm": "corepack enable && corepack install",
		"test": "vitest --ui",
		"prettier": "prettier --write .",
		"git:fetch": "git fetch -p",
		"git:dev-2-main": "git rebase dev main",
		"git:main-2-dev": "git rebase main dev",
		"up-ruan-cat": "pnpm up @ruan-cat/*@latest",
		"update-package": "pnpm dlx vp-update",
		"turbo:update-packages": "turbo docs:update-package && pnpm update-package",
		"rm:node_modules": "pnpm -r rm:node_modules && rimraf node_modules pnpm-lock.yaml",
		"clear": "rimraf dist .turbo .vercel .cache .temp",
		"pnpm:clear": "pnpm -r clear && pnpm run clear",
		"turbo:build-docs": "turbo build:docs",
		"on-github-action-flow": "pnpm turbo:build-docs",
		"code-workspace:create": "node --import=tsx ./scripts/generate.code-workspace.ts --name=note笔记项目",
		"deploy-vercel": "node --import=tsx ./scripts/vercel-deploy-tool.ts"
	},
	"dependencies": {
		"@vercel/analytics": "^1.4.1",
		"@vueuse/core": "^10.11.1",
		"animate.css": "^4.1.1",
		"axios": "^1.7.8",
		"dayjs": "^1.11.13",
		"element-plus": "^2.9.0",
		"lodash-es": "^4.17.21",
		"node-localstorage": "^2.2.1",
		"pinia": "^2.2.8",
		"pinia-plugin-persistedstate": "^3.2.3",
		"uuid": "^9.0.1",
		"vue": "^3.5.13",
		"vue-json-excel3": "^0.0.9",
		"xlsx": "^0.18.5"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.6.0",
		"@commitlint/config-conventional": "^19.6.0",
		"@commitlint/lint": "^19.6.0",
		"@commitlint/types": "^19.5.0",
		"@iconify/json": "^2.2.278",
		"@iconify/vue": "^4.1.2",
		"@ruan-cat/domains": "^0.1.3",
		"@ruan-cat/generate-code-workspace": "^0.2.0",
		"@ruan-cat/utils": "^1.3.0",
		"@ruan-cat/vercel-deploy-tool": "^0.5.0",
		"@ruan-cat/vuepress-preset-config": "^0.1.14",
		"@types/js-yaml": "^4.0.9",
		"@types/lodash-es": "^4.17.12",
		"@types/node": "^20.17.9",
		"@types/uuid": "^9.0.8",
		"@vitejs/plugin-vue": "^4.6.2",
		"@vitest/ui": "^2.1.7",
		"@vuepress/bundler-vite": "2.0.0-rc.18",
		"@vuepress/plugin-docsearch": "2.0.0-rc.61",
		"@vuepress/plugin-register-components": "2.0.0-rc.54",
		"cpx": "^1.5.0",
		"cross-env": "^7.0.3",
		"cz-git": "^1.11.0",
		"glob": "^11.0.0",
		"js-yaml": "^4.1.0",
		"mermaid": "^10.9.3",
		"mkdirp": "^3.0.1",
		"moment": "^2.30.1",
		"only-allow": "^1.2.1",
		"pkg-types": "^1.2.1",
		"prettier": "^3.4.1",
		"prettier-plugin-lint-md": "^1.0.1",
		"rimraf": "^6.0.1",
		"sass-embedded": "^1.81.0",
		"shx": "^0.3.4",
		"tsx": "^4.19.2",
		"turbo": "^2.3.3",
		"typedoc": "^0.25.13",
		"typedoc-plugin-markdown": "^3.17.1",
		"typescript": "^5.7.2",
		"unplugin-auto-import": "^0.18.6",
		"unplugin-vue-components": "^0.27.5",
		"vercel": "37.12.1",
		"vite": "^5.4.11",
		"vite-node": "^1.6.0",
		"vitest": "^2.1.7",
		"vuepress": "2.0.0-rc.18",
		"vuepress-plugin-md-enhance": "2.0.0-rc.59",
		"vuepress-plugin-typedoc": "0.13.0",
		"vuepress-theme-hope": "2.0.0-rc.59"
	}
}
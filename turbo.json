{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build:docs": {
			"cache": true,
			"outputs": [
				"**/dist/**",
				"**/.vitepress/dist/**",
				"**/.vuepress/dist/**"
			],
			"dependsOn": [
				"^build:docs"
			]
		},
		"@ruan-cat-docs/notes#build": {},
		"@ruan-cat-docs/rpgmv-dev-notes#build": {},
		"@ruan-cat-docs/docs-01-star#build": {},
		"//#do-build": {
			"cache": true,
			"outputs": [
				"**/dist/**",
				"**/.vitepress/dist/**",
				"**/.vuepress/dist/**"
			],
			"dependsOn": [
				"@ruan-cat-docs/notes#build",
				"@ruan-cat-docs/rpgmv-dev-notes#build",
				"@ruan-cat-docs/docs-01-star#build"
			]
		},
		"//#deploy-on-vercel": {
			"dependsOn": [
				"//#do-build"
			]
		},
		"//#do-deploy": {
			"dependsOn": [
				"//#deploy-on-vercel"
			]
		},
		"docs:update-package": {
			"cache": false,
			"dependsOn": []
		},
		"clear": {
			"dependsOn": [
				"^clear"
			]
		},
		"do-build-note-doc": {
			"cache": true,
			"outputs": [
				"docs/ruan-cat-notes/docs/.vitepress/dist/**"
			],
			"dependsOn": [
				"@ruan-cat-docs/notes#build:docs"
			]
		},
		"do-build-01star-doc": {
			"cache": true,
			"outputs": [
				"docs/docs-01-star/docs/.vitepress/dist/**"
			],
			"dependsOn": [
				"@ruan-cat-docs/docs-01-star#build:docs"
			]
		}
	}
}
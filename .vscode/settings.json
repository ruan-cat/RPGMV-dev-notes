{
	"[vue]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"picgo": {
		"picBed": {
			"current": "github",
			"uploader": "github",
		},
	},
	"code-runner.executorMap": {
		// "typescript": "node --loader ts-node/esm $fullFileName --files"
		// "typescript": "npx vite-node $fullFileName --files"
		// "typescript": "pnpx vite-node $fullFileName --files"
		// 似乎快了一点点 3秒左右吧
		"typescript": "pnpm vite-node $fullFileName --files"
		// "typescript": "pnpm vite-node $fullFileName"
	},
	"todo-tree.tree.disableCompactFolders": false,
	"todo-tree.tree.showCountsInTree": true,
	"todo-tree.tree.showBadges": true,
	"vue.codeActions.enabled": false,
	"svn.ignoreMissingSvnWarning": true,
	// 尝试实现指定文本的屏蔽 但是无效
	"todo-tree.highlights.customHighlight": {
		"参考资料": {
			"type": "none"
		},
	},
	// 文件打开的默认行为
	"workbench.editorAssociations": {
		// "*.svg": "hediet.vscode-drawio-text",
		"*.svg": "hediet.vscode-drawio",
		// "*.drawio.svg": "hediet.vscode-drawio",
	},
}
{
	"[vue]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"picgo": {
		"picBed": {
			"current": "github",
			"uploader": "github",
		}
	},
	"code-runner.executorMap": {
		"typescript": "node --import=tsx $fullFileName"
	},
	"todo-tree.tree.disableCompactFolders": false,
	"todo-tree.tree.showCountsInTree": true,
	"todo-tree.tree.showBadges": true,
	"vue.codeActions.enabled": false,
	"svn.ignoreMissingSvnWarning": true,
	// 尝试实现指定文本的屏蔽 但是无效
	"todo-tree.highlights.customHighlight": {
		"参考资料": {
			"type": "none"
		}
	},
	// 文件打开的默认行为
	"workbench.editorAssociations": {
		// "*.svg": "hediet.vscode-drawio-text",
		// "*.drawio.svg": "hediet.vscode-drawio",
		// 正常可用
		"*.drawio.svg": "hediet.vscode-drawio-text"
	},
	/** 
		让volar对md提供基础的类型支持
		- https://github.com/vuejs/language-tools/tree/master/extensions/vscode#configs
		- https://vitepress.dev/guide/using-vue#vs-code-intellisense-support
	*/
	"vue.server.includeLanguages": [
		"vue",
		"markdown"
	]
}